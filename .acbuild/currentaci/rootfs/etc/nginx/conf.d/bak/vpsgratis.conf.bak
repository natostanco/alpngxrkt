#
# The default server
#
server {
    listen       80;
    server_name  www.vpsgratis.com vpsgratis.com m.vpsgratis.com;

    #charset koi8-r;

   location / {
        root   /usr/share/nginx/html/vpsgratis;
        index  index.php index.html index.htm;
	include conf.d/rewrites.nix;
   }
   include conf.d/w3cacherec.nix;
   include conf.d/files.nix;
   include conf.d/php-server.nix;
}



